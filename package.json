{
    "name": "appbegomentalreset",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "clean": "turbo run clean",
        "type-check": "turbo run type-check",
        "dev:web": "turbo run dev --filter=web",
        "dev:cms": "turbo run dev --filter=cms",
        "build:web": "turbo run build --filter=web",
        "build:cms": "turbo run build --filter=cms"
    },
    "devDependencies": {
        "turbo": "^1.13.0",
        "typescript": "^5.0.0"
    },
    "packageManager": "pnpm@8.15.0",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    }
}